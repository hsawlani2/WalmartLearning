<apex:page standardStylesheets="false"
  controller="FormSuccessPageController"
  showHeader="false"
  sidebar="false"
  applyBodyTag="false"
  applyHtmlTag="false"
  docType="html-5.0">
    
    <html>
        <head>
            <meta charset="utf-8" />
            <meta http-equiv="X-UA-Compatible" content="IE=edge" />
            <meta name="viewport" content="width=device-width, initial-scale=1" />
            <title>Seller Questionnaire</title>
            
            <!-- Required to run anything else! -->
            <script src="{!$Resource.pkb_jquery_1_6_2}" type="text/javascript"></script>
        
            <!-- Global stylesheets -->
            <apex:stylesheet value="{!URLFOR($Resource.QuestionnaireAssets, 'assets/css/icons/icomoon/styles.css') }"/>
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css" />
            <apex:stylesheet value="{!URLFOR($Resource.QuestionnaireAssets, 'assets/css/bootstrap.min.css') }"/>
            <apex:stylesheet value="{!URLFOR($Resource.QuestionnaireAssets, 'assets/css/core.min.css') }"/>
            <apex:stylesheet value="{!URLFOR($Resource.QuestionnaireAssets, 'assets/css/components.min.css') }"/>
            <apex:stylesheet value="{!URLFOR($Resource.QuestionnaireAssets, 'assets/css/colors.min.css') }"/>
            <!-- Global stylesheets -->
            
            <!-- custom overrides -->
            <c:questionnaire_css_overrides />
            
        
        </head>
        
        <body>
            
            <div class="navbar navbar-inverse">
                <div class="navbar-header">
                    <a class="navbar-brand" href="/questionnaire">
                        <apex:image url="{!URLFOR($Resource.WMPLogo)}" />
                    </a>
                </div>
            </div>
            
            <!-- Page container -->
            <div class="page-container">
        
                <!-- Page content -->
                <div class="page-content">
        
                    <!-- Main content -->
                    <div class="content-wrapper message-page-body">
                        
                        <h1>{!pCon.pageTitle}</h1>
                    
                        <p>{!pCon.pageMessage}</p>
                        <p>{!pCon.pageErrorDetails}</p>
                        
                    </div>
                </div>
            </div>
            
            <script src="{!URLFOR($Resource.QuestionnaireAssets, 'assets/js/core/libraries/jquery.min.js') }" type="text/javascript" />
            <script src="{!URLFOR($Resource.QuestionnaireAssets, 'assets/js/core/libraries/bootstrap.min.js') }" type="text/javascript" />
            <script src="{!URLFOR($Resource.QuestionnaireAssets, 'assets/js/plugins/loaders/blockui.min.js') }" type="text/javascript" />
            <script src="{!URLFOR($Resource.QuestionnaireAssets, 'assets/js/plugins/ui/drilldown.js') }" type="text/javascript" />
            <script src="{!URLFOR($Resource.QuestionnaireAssets, 'assets/js/plugins/ui/nicescroll.min.js') }" type="text/javascript" />
            <script src="{!URLFOR($Resource.QuestionnaireAssets, 'assets/js/plugins/loaders/pace.min.js') }" type="text/javascript" />
            <script src="{!URLFOR($Resource.QuestionnaireAssets, 'assets/js/plugins/forms/selects/select2.min.js') }" type="text/javascript" />
            <script src="{!URLFOR($Resource.QuestionnaireAssets, 'assets/js/plugins/forms/styling/uniform.min.js') }" type="text/javascript" />
            <script src="{!URLFOR($Resource.QuestionnaireAssets, 'assets/js/core/libraries/jasny_bootstrap.min.js') }" type="text/javascript" />
            <script src="{!URLFOR($Resource.QuestionnaireAssets, 'assets/js/plugins/forms/validation/validate.min.js') }" type="text/javascript" />
            <script src="{!URLFOR($Resource.QuestionnaireAssets, 'assets/js/plugins/extensions/cookie.js') }" type="text/javascript" />
            <script src="{!URLFOR($Resource.QuestionnaireAssets, 'assets/js/core/libraries/jquery_ui/interactions.min.js') }" type="text/javascript" />
            <script src="{!URLFOR($Resource.QuestionnaireAssets, 'assets/js/plugins/forms/tags/tagsinput.min.js') }" type="text/javascript" />
            <script src="https://rawgit.com/leo-fcx/jquery-steps/master/build/jquery.steps.js"></script>
            
            <script src="{!URLFOR($Resource.QuestionnaireAssets, 'assets/js/core/app.js') }" type="text/javascript" />
            
            
        </body>
        
    </html>
</apex:page>