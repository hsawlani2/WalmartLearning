            <script>
                $(function() {
                    init();
                });
                
                function init(){
                    $(".select").multiselect({
                        selectedList: 5
                    });
                    
                    $('.required').unbind('change');
                    $(".required").change(function(){
                        var val = $(this).val();
                        if(val.trim() != ''){
                            $(this).removeClass('err');
                        }else{
                            $(this).addClass('err');
                        }
                    });
                    
                    $('.ctn').unbind('click');
                    $('.ctn').click(function(){
                        var currentTR = $(this).closest('tr');
                        var prevTR = $(this).closest('tr').prev('tr');
                    
                        $(currentTR).find('.fname').val($(prevTR).find('.fname').val());
                        $(currentTR).find('.lname').val($(prevTR).find('.lname').val());
                        $(currentTR).find('.email').val($(prevTR).find('.email').val());
                        $(currentTR).find('.phone').val($(prevTR).find('.phone').val());
                        $(currentTR).find('.ck').prop('checked', $(prevTR).find('.ck').prop('checked'));
                        
                        var vl = $(prevTR).find('.select').attr('val');
                        var vlArr = vl.split(';');
                        
                        /*$(currentTR).find('.select').find('option').each(function(){
                            var found = $.inArray($(this).val(), vlArr) > -1;
                            if(found)
                                $(this).attr("selected", 1);
                        });*/
                    });
                }
                
                function disableButtons(){
                    $('.btn-blue').prop('disabled', true);
                    $('.btn-blue').val('processing..');
                    $('.btn-blue span').text('processing..');
                }
                
                function checkEmail(obj){
                    var val = $(obj).val();
                    var tr = $(obj).closest('tr');
                    
                    if(val.trim() != ''){
                        if(validateEmail(val)){
                            $(obj).removeClass('err');
                        }else
                        {
                            $(obj).addClass('err');
                        }
                    }
                    
                    var isValid = true;
                    $(tr).find('.required').each(function(){
                        if($(this).val().trim() == '')
                            isValid = false;
                    });
                    
                    if(isValid)
                        $(tr).find('.ck').prop('checked', true);
                }
                
                function checkPhone(obj){
                    var val = $(obj).val();
                    if(val.trim() != ''){
                        if(validatePhone(val)){
                            $(obj).removeClass('err');
                        }else
                        {
                            $(obj).addClass('err');
                        }
                    }
                }
                
                function checkRequired(obj){
                    var val = $(obj).val();
                    var tr = $(obj).closest('tr');
                    
                    if(val.trim() == ''){
                        $(obj).addClass('err');
                        $(obj).attr('placeHolder', 'Please enter a value');
                    }else{
                        $(obj).removeClass('err');
                        $(obj).attr('placeHolder', '');
                    }
                    
                    var isValid = true;
                    $(tr).find('.required').each(function(){
                        if($(this).val().trim() == '')
                            isValid = false;
                    });
                    
                    if(isValid)
                        $(tr).find('.ck').prop('checked', true);
                }
                
                function validateEmail(email) {
                    var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
                    return re.test(email);
                }
                
                function validatePhone(phone){
                    var RE = /^[\d\.\-]+$/;
                    if(!RE.test(phone) || phone.trim().length != 10)
                    {
                        return false;
                    }
                    return true;
                }
                
                function validateForm(){
                    var isValid = true;
                    $('.fname, .lname').each(function(){
                        var val = $(this).val();
                        if(val.trim() == ''){
                            isValid = false;
                            $(this).addClass('err');
                            $(this).attr('placeHolder', 'Please enter a value');
                        }else{
                            $(this).removeClass('err');
                            $(this).attr('placeHolder', '');
                        }
                    });
                    
                    $('.email').each(function(){
                        var val = $(this).val();
                        if(val.trim() != ''){
                            if(validateEmail(val)){
                                $(this).removeClass('err');
                            }else
                            {
                                isValid = false;
                                $(this).addClass('err');
                            }
                        }else{
                            $(this).attr('placeHolder', 'Please enter a value');
                            isValid = false;
                            $(this).addClass('err');
                        }
                    });
                    
                    $('.phone').each(function(){
                        var pval = $(this).val();
                        if(pval.trim() != ''){
                            if(validatePhone(pval)){
                                $(this).removeClass('err');
                            }else
                            {
                                isValid = false;
                                $(this).addClass('err');
                            }
                        }
                    });
                    
                    $(".required").each(function(){
                        var rval = $(this).val();
                        if(rval.trim() == ''){
                            isValid = false;
                            $(this).addClass('err');
                        }else{
                            $(this).removeClass('err');
                        }
                    });
                    
                    if(isValid){
                        var address = $('.street').val()+', '+$('.city').val()+', '+$('[id$="state"]').val()+', '+$('.zip').val()+', '+$('.country').val();
                        console.log(address);
                        
                        // Create a new Geocoder
                        var geocoder = new google.maps.Geocoder();
                        // Locate the address using the Geocoder.
                        geocoder.geocode( { "address": address }, function(results, status) {
                            // If the Geocoding was successful
                            if (status == google.maps.GeocoderStatus.OK) {
                                console.log('result ' + results[0].geometry.location);
                                $('.errorM4Td').hide();
                            }else{
                                $('.errorM4Td').show();
                                isValid = false;
                            }
                        });
                    }
                    
                    if(isValid){
                        $('.select').each(function(){
                            var vl = $(this).val();
                            $(this).closest('td').find('input:hidden').val(vl);
                        });
                        disableButtons();
                        callSave();
                    }
                }
                
                function calAdd(){
                    $('.select').each(function(){
                        var vl = $(this).val();
                        $(this).closest('td').find('input:hidden').val(vl);
                    });
                    callAdd();
                }
                
                function jsRemoveRw(vl){
                    $('.select').each(function(){
                        var vl = $(this).val();
                        $(this).closest('td').find('input:hidden').val(vl);
                    });
                    jsRemoveRow(vl);
                }
            </script>
